model Wire {
  id String @id @default(uuid())

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  circleId          String @unique
  circleFingerprint String

  description String

  userId           String
  bankDetailsId    String
  billingDetailsId String

  user    User @relation(fields: [userId], references: [id])

  bank    WireBankDetail @relation(fields: [bankDetailsId], references: [id])
  billing BillingDetail  @relation(fields: [billingDetailsId], references: [id])

  payouts Payout[]
}

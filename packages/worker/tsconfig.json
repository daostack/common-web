{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "noImplicitReturns": true,
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "strict": true,
    "declaration": true,
    "paths": {
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}

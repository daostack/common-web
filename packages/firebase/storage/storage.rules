rules_version = '2';
service firebase.storage {
	
  match /public_img/{allPaths=**} {
  	allow read;
    allow write: if request.auth != null;
  }
  match /public_file/{allPaths=**} {
  	allow read;
    allow write: if request.auth != null;
  }
  match /{allPaths=**} {
      allow read, write;
  }
  
}
